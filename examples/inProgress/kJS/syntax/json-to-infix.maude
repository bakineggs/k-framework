 	eq o(type :  "PLUS" children :  a(Left , Right) Properties) =  add(Left , Right) [metadata "location(json-to-infix.k:18)"] .
 	
 	eq o(type :  "INCREMENT" children :  a(Element:JS) Properties) =  inc(Element, false) [metadata "location(json-to-infix.k:20)"] .
