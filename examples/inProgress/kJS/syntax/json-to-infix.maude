 	  sort JsonTag .    op type :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op children :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op name :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op value :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op expression :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op assignOp :  -> JsonTag   [metadata "location(json-to-infix.k:6)"] .  op postfix :  -> JsonTag [metadata "location(json-to-infix.k:6)"] .  
 	
 	      subsort JsonTag <  Id .  
 	
 	eq o(type :  "NUMBER" value :  N:Numeric Properties:JSProperties) =  n(N) [metadata "location(json-to-infix.k:10)"] .
 	eq o(type :  "STRING" value :  S:String Properties) =  s(S) [metadata "location(json-to-infix.k:11)"] .
 	eq o(type :  "BOOLEAN" value :  B:Bool Properties) =  b(B) [metadata "location(json-to-infix.k:12)"] .
 	
 	eq o(type :  "IDENTIFIER" name :  Name:String Properties) =  id(Name) [metadata "location(json-to-infix.k:14)"] .
 	
