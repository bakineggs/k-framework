 	eq o(type :  "SCRIPT" children :  a(Elements:List{JS}) Properties) =  program(Elements) [metadata "location(json-to-infix.k:27)"] .
 	
 	eq o(type :  "VAR" children :  a(Elements) Properties) =  varDeclarations(Elements) [metadata "location(json-to-infix.k:29)"] .
 	
 	eq o(type :  "SEMICOLON" expression :  a(Elements) Properties) =  stmts(Elements) [metadata "location(json-to-infix.k:31)"] .
 	
 	eq o(type :  "DOT" children :  a(Left , Right) Properties) =  staticAccess(Left , Right) [metadata "location(json-to-infix.k:33)"] .
 	
